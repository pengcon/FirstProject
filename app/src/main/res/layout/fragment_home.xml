<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout

            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/Toolbar"
            android:layout_width="match_parent"
            android:layout_height="56dp"

            android:gravity="center"
            android:background="@drawable/ic_frame_14"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"

            >
            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="52dp"
                android:layout_height="30dp"

                app:srcCompat="@drawable/ic_logo" />

        </RelativeLayout>
        <ImageView
            android:id="@+id/Ai"
            android:layout_width="match_parent"
            android:layout_height="61dp"
            android:layout_gravity="center"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            app:layout_constraintTop_toBottomOf="@id/Toolbar"
            app:srcCompat="@drawable/ic_group_11__1_" />
        <ImageView
            android:id="@+id/banner"
            android:layout_width="match_parent"
            android:layout_height="173dp"
            android:layout_gravity="center"
            android:layout_marginTop="8dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            app:layout_constraintTop_toBottomOf="@id/Ai"
            app:srcCompat="@drawable/banner" />
        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_navi"
            app:layout_constraintTop_toBottomOf="@id/banner"/>
        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_height="13dp"
            android:layout_gravity="center"
            app:srcCompat="@drawable/rectangle"
            app:layout_constraintTop_toBottomOf="@id/imageView4"/>
        <TextView
            android:id="@+id/sukso"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosanskr_medium"
            android:layout_marginTop="5dp"
            android:includeFontPadding="false"
            android:text="휴가를 책임질 한국관광공사 추천 여행코스 "
            android:textColor="@color/black"
            android:textSize="18sp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="5dp"
            app:layout_constraintTop_toBottomOf="@id/imageView3"
            android:textStyle="normal"/>
        <HorizontalScrollView
            android:id="@+id/suksoview"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:orientation="horizontal"
            android:scrollbars="none"
            app:layout_constraintTop_toBottomOf="@id/sukso">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/border"

                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/sukso1"
                        android:layout_width="170dp"
                        android:layout_height="120dp"
                        android:scaleType="centerCrop"

                        android:src="@drawable/incheon"/>
                    <TextView
                        android:id="@+id/title1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="8dp"
                        android:textSize="12sp"
                        android:text="화려하게 빛나는\n인천의 낮과 밤"
                        android:fontFamily="@font/notosanskr_bold"
                        android:textColor="#FF000000"
                        android:includeFontPadding="false"
                        android:gravity="top"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/address_marker"
                            android:layout_width="9dp"
                            android:layout_height="9dp"

                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/ic___icon__map_marker_"/>
                        <TextView
                            android:id="@+id/address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:layout_marginBottom="5dp"
                            android:textSize="8sp"
                            android:text="인천광역시 중구"


                            />
                    </LinearLayout>



                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/border"

                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/sukso2"
                        android:layout_width="170dp"
                        android:layout_height="120dp"
                        android:scaleType="centerCrop"

                        android:src="@drawable/yunsundo"/>
                    <TextView
                        android:id="@+id/title2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="8dp"
                        android:textSize="12sp"
                        android:text="고산 윤선도의\n이상향을 만나다"
                        android:fontFamily="@font/notosanskr_bold"
                        android:textColor="#FF000000"
                        android:includeFontPadding="false"
                        android:gravity="top"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/address_marker2"
                            android:layout_width="9dp"
                            android:layout_height="9dp"

                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/ic___icon__map_marker_"/>
                        <TextView
                            android:id="@+id/address2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:layout_marginBottom="5dp"
                            android:textSize="8sp"
                            android:text="전라남도 완도군"


                            />
                    </LinearLayout>



                </LinearLayout>
            </LinearLayout>









        </HorizontalScrollView>
        <TextView
            android:id="@+id/sukso6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/notosanskr_medium"
            android:layout_marginTop="5dp"
            android:includeFontPadding="false"
            android:text="대충 소상공인 장소 추천한다는 내용  "
            android:textColor="@color/black"
            android:textSize="18sp"
            android:layout_marginLeft="10dp"
            android:layout_marginBottom="5dp"
            app:layout_constraintTop_toBottomOf="@id/suksoview"
            android:textStyle="normal"/>
        <HorizontalScrollView
            android:id="@+id/suksoview2"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:orientation="horizontal"
            android:scrollbars="none"
            app:layout_constraintTop_toBottomOf="@id/sukso6">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/border"

                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/sukso3"
                        android:layout_width="170dp"
                        android:layout_height="120dp"
                        android:scaleType="centerCrop"

                        android:src="@drawable/incheon"/>
                    <TextView
                        android:id="@+id/title3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="8dp"
                        android:textSize="12sp"
                        android:text="화려하게 빛나는\n인천의 낮과 밤"
                        android:fontFamily="@font/notosanskr_bold"
                        android:textColor="#FF000000"
                        android:includeFontPadding="false"
                        android:gravity="top"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/address_marker3"
                            android:layout_width="9dp"
                            android:layout_height="9dp"

                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/ic___icon__map_marker_"/>
                        <TextView
                            android:id="@+id/address3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:layout_marginBottom="5dp"
                            android:textSize="8sp"
                            android:text="인천광역시 중구"


                            />
                    </LinearLayout>



                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/border"

                    android:layout_marginTop="10dp">
                    <ImageView
                        android:id="@+id/sukso4"
                        android:layout_width="170dp"
                        android:layout_height="120dp"
                        android:scaleType="centerCrop"

                        android:src="@drawable/yunsundo"/>
                    <TextView
                        android:id="@+id/title4"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="1dp"
                        android:layout_marginLeft="8dp"
                        android:textSize="12sp"
                        android:text="고산 윤선도의\n이상향을 만나다"
                        android:fontFamily="@font/notosanskr_bold"
                        android:textColor="#FF000000"
                        android:includeFontPadding="false"
                        android:gravity="top"/>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/address_marker4"
                            android:layout_width="9dp"
                            android:layout_height="9dp"

                            android:layout_marginLeft="8dp"
                            android:layout_marginTop="2dp"
                            android:src="@drawable/ic___icon__map_marker_"/>
                        <TextView
                            android:id="@+id/address4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="2dp"
                            android:layout_marginBottom="5dp"
                            android:textSize="8sp"
                            android:text="전라남도 완도군"


                            />
                    </LinearLayout>



                </LinearLayout>
            </LinearLayout>









        </HorizontalScrollView>


    </androidx.constraintlayout.widget.ConstraintLayout>












</ScrollView>
